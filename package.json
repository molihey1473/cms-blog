    "html-react-parser": "^1.4.0",
    "next": "11.1.2",
    "node-fetch": "3.2.3",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rss-parser": "^3.12.0",
    "sass": "^1.32.8",
    "twemoji": "^13.1.0",
    "twemoji-parser": "^14.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
